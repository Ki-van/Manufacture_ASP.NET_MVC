@model IEnumerable<Manufacture_ASP.NET_MVC.Models.ComplitedStatus>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Status">Редактировать</a> |
                <a asp-action="Details" asp-route-id="@item.Status">Обзор</a> |
                <a asp-action="Delete" asp-route-id="@item.Status">Удалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
