@model KP_MANUFACTURE_MVC.Models.Export

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Export</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ProductCount" class="control-label"></label>
                <input asp-for="ProductCount" class="form-control" />
                <span asp-validation-for="ProductCount" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="Product" class="control-label"></label>
                <select asp-for="Product" class="form-control" asp-items="ViewBag.ProductName"></select>
            </div>
            <div class="form-group">
                <label asp-for="Importer" class="control-label"></label>
                <select asp-for="Importer" class="form-control" asp-items="ViewBag.ImporterFullName"></select>
            </div>
            <div class="form-group">
                <label asp-for="ComplitedStatusId" class="control-label"></label>
                <select asp-for="ComplitedStatusId" class="form-control" asp-items="ViewBag.ComplitedStatusId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
